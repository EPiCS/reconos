BUS_INTERFACE BUS=HWIF, BUS_STD=HWIF_STD, BUS_TYPE=TARGET

BUS_INTERFACE BUS=SFIFO32_MEMCTRL, BUS_STD=SFIFO32_STD, BUS_TYPE=INITIATOR
