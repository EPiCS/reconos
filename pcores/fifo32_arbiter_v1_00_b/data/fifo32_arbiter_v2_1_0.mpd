PORT HWIF2DEC_RdCE = IP2HWT_RdCE, DIR= I,                                    BUS= HWIF
PORT HWIF2DEC_WrCE = IP2HWT_WrCE, DIR= I, 				     BUS= HWIF
PORT DEC2HWIF_Data = HWT2IP_Data, DIR= O, VEC= [31:0], BUS= HWIF
PORT DEC2HWIF_RdAck= HWT2IP_RdAck,DIR= O,                                    BUS= HWIF
